<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="BrightLauncher.Views.Download.Other.NewsView">
	<Panel>
		<ScrollViewer>
			<StackPanel Name="newslist" Orientation="Vertical" Margin="20,10,20,0"/>
		</ScrollViewer>
		<StackPanel IsHitTestVisible="False" Name="Isok" VerticalAlignment="Center" HorizontalAlignment="Center">
			<ProgressBar Classes="Circle" CornerRadius="20" IsIndeterminate="True" Height="10" Width="100"/>
			<TextBlock Margin="0,10,0,0" TextAlignment="Center" Text="加载中" Opacity="0.85" FontSize="18"/>
		</StackPanel>
	</Panel>
</UserControl>
